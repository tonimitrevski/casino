  "moduleDirectories": ["node_modules", "src"],
  "moduleNameMapper": {
    "^worker-plugin/loader.+$": "<rootDir>/src/redux/Services/fetch-games-and-jack-pots/getJackpotsAndGamesWorker.worker.ts"
