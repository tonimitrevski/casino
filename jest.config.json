{
  "verbose": true,
  "setupFiles": ["<rootDir>/src/setupTests.ts"],
  "collectCoverage": true,
  "coverageReporters": ["lcov"],
  "coverageDirectory": "test-coverage",
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
